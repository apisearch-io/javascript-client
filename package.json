{
  "name": "es6-lib-skeleton",
  "version": "1.0.0",
  "description": "Skeleton for javascript es6 library development",
  "main": "index.js",
  "license": "MIT",
  "author": "alexhoma <alexcm.14@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexhoma/es6-lib-skeleton.git"
  },
  "keywords": [
    "es6",
    "skeleton",
    "library",
    "webpack"
  ],
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "jest-cli": "^21.0.2",
    "webpack": "^3.5.5",
    "yargs": "^8.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "rootDir": "",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "<rootDir>/test"
    ]
  }
