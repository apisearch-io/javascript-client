{
  "name": "apisearch",
  "version": "0.1.22",
  "description": "Javascript client for Apisearch.",
  "main": "./dist/apisearch.js",
  "scripts": {
    "dev": "webpack --config ./webpack.dev.js --progress --colors --watch",
    "build:dev": "webpack --config ./webpack.dev.js",
    "build:min": "webpack --config ./webpack.min.js",
    "dist": "npm t; rm -rf ./dist/*; npm run build:dev && npm run build:min",
    "test": "mocha --recursive --require ts-node/register test/*/*.ts",
    "test:functional": "mocha --recursive --require ts-node/register test/Functional/*/*.ts"
  },
  "license": "MIT",
  "author": "alexhoma <alexcm.14@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/apisearch/javascript-search-client.git"
  },
  "bugs": {
    "url": "https://github.com/apisearch/javascript-search-client/issues"
  },
  "homepage": "https://github.com/apisearch/javascript-search-client",
  "keywords": [
    "es6",
    "search-engine",
    "apisearch",
    "webpack4",
    "node",
    "javascript-client"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.0",
    "typings": "^2.1.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "yargs": "^8.0.2",
    "tslint": "^5.10.0"
  },
  "dependencies": {
    "@types/node": "^10.5.1",
    "axios": "^0.17.1",
    "ts-loader": "^4.4.2",
    "ts-md5": "^1.2.4"
  }
}
